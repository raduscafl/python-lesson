    print(x.isupper())
    if x.isupper():
        count += 1
print(count)


words='CAPS LOCK is a nice key!'
count = 0
words=input()
listos=list(words)
for x in set(listos):
    if x.isupper():
        count += 1
print(count)

sentence=[]
with open(r'file1.txt', 'r', encoding='utf-8') as input_file:
    with open(r'file2.txt', 'w', encoding='utf-8') as output_file:
        for x in input_file:
            x=x.strip()
            sentence.append(x)
        sentence=sorted(sentence)
        for line in sentence:
            output_file.write(line + '\n')

listos=[]
words=[]
capwordss=[]
mergedlist=[]
with open(r'file1.txt', 'r', encoding='utf-8') as input_file:
    for q in input_file:
        mergedlist=q.strip().split()
        words=mergedlist+words
    for word in words:
        if word[0].isupper():
            listos.append(word)
    for capwords in listos:
        capwordss.append(capwords)
    for a in sorted(set(capwordss)):
        print(a)




