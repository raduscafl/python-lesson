with open(r'file1.txt', 'r', encoding='utf-8') as input_file:
    with open('file2.txt', 'w',encoding='utf-8') as output_file:
        for line in input_file:
            try:
                line = line.strip()
                line=int(line)
                output_file.write(line + '\n')
                continue
            except:
                line=str(line)


total=0
n=input()
n=int(n)
for x in range(0,n):
    x=int(input())
    if x%2==0:
        total=x+total
    else:
        pass
print(total)


