def is_palindrome(word):
    if word == word[::-1]:
        return True
    else:
        return False

count=0
words=[]
sentence='lol a test of palidromes we have toot'
words=sentence.split()
for word in words:
    if word == word[::-1]:
        count += 1
    else:
        pass
print(count)


