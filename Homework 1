names = ['Alex', 'Bob', 'Claudia', 'Daniel', 'Eugene', 'Felix', 'George']
for i, n in enumerate(names):
    if (i % 2==0):
        print(n)

names = ['Alex', 'Bob', 'Claudia', 'Daniel', 'Eugene', 'Felix', 'George']
filtered_names = [ name for name in names if len(name)>=4 ]

for n in filtered_names:
    print(n)


