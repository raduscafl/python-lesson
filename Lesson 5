    print('Three')
    print('Four')

x=[1,3,7,99]
print( [ for number in x number if number > 0 ] )
print( [ number for number in x if number > 0 ] )
print( [ number for number in x ] )
print( [ for number in x if number > 0 number ] )
print( [ number if number > 0 for number in x ] )



