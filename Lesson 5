try:
    y = [ 10 / x for x in range(10) ]
except ZeroDivisionError:
    print('We’ve intercepted the error so that now our program won’t crash!')


user_list = [ -100, 4, 90, 32, -10, -3, 0, 17, 67 ]
print('We’re very proud having only positive feedback:')
for score in user_list:
    if score> 50:
        print(score)

greeting = 'Hi there'
for i in range(100):
    greeting = greeting + '!'

print(greeting)


y = [int(x) for x in input().split()]
q=sorted(y)
for z in q:
    print(z, end=' ')

my_list=input().split()
int_list=sorted([int(x) for x in my_list])
for x in int_list:
    print(x,end=' ')





def estimate(x):
    s='small'
    l='LARGE'
    if x<100:
        return s
    else:
        return l


print(estimate(int(input())))





def keep_positive(a):
    b=[x for x in a if x>0]
    return b

print(
    ' '.join(list(map(
        str,
        keep_positive(map(
            int,
            input().split()
        ))
    )))
)

